<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="budgetPlanner.model">
	<class name="IncomePay" table="Incomepay">
		<meta attribute="class-description">
			This class contains the employee detail.
		</meta>
		<id name="userId" type="int" column="id">
		</id>
		<property name="yourPay" column="yourPay" type="double" />
		<property name="partnersPay" column="partnersPay" type="double" />
		<property name="bonuses" column="bonuses" type="double" />
		<property name="familyPayments" column="familyPayments" type="double" />
		<property name="other" column="other" type="double" />
	</class>
</hibernate-mapping>